package Batybank_herdado;

public class FuncionarioAutenticavel extends Funcionario {
	
	private int senha;
	
	
	/**--------------SENHA--------------------*/
	public boolean autentica(int senha) {
		if (this.senha == senha) {
			return true;
		} else {
			return false;
		}
	}
	
	public void setSenha(int senha) {
		this.senha = senha;
	}

	@Override
	public double getBonificacao() {
		
		return 0;
	}

}
