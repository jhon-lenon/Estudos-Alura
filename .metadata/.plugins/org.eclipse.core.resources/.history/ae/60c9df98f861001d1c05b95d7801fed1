package Batybank_herdado;

public class Gerente {
	private int senha;
	
	
	/**--------------SENHA--------------------*/
	public boolean autentica(int senha) {
		if (this.senha == senha) {
			return true;
		} else {
			return false;
		}
	}

	/**-----------BONIFICAÇÃO-----------------*/
	public double getBonificacao() {
		return this.salario;
	}
	
	/**---------------------------------------*/
	

}
