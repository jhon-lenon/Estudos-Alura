package Batybank_herdado;

public class Gerente extends Funcionario {
	private int senha;
	
	
	/**--------------SENHA--------------------*/
	public boolean autentica(int senha) {
		if (this.senha == senha) {
			return true;
		} else {
			return false;
		}
	}

